"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace:该mapper.xml映射文件的 唯一标识 -->
<mapper namespace="org.ljn.mapper.StudentMapper">
	
	<!-- <select id="queryStudentByStuno" 	parameterType="int"  	resultType="org.ljn.entity.Student"  >
		select * from student where stuno = #{stuNo}
	</select> -->
	
	
	<insert id="addStudent" parameterType="org.ljn.entity.Student" >
		insert into student(number,name,age) values(#{number},#{name},#{age})
	</insert>
	
</mapper>